#import "UIControl+ZCDataOutlet.h"

@implementation UIControl (ZCDataOutlet)

-(BOOL) isDisabled{
    return ![self isEnabled];
}

-(void) setDisabled:(BOOL)disabled{
    [self setEnabled:!disabled];
}

@end
